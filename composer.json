{
    "name": "xiaohuilam/laravel-socialite-skeleton",
    "description": "Laravel to support multiple socialization login implment scheleton",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaohui.lam",
            "email": "xiaohui.lam@icloud.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Xiaohuilam\\Laravel\\SocialiteSkeleton\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "suggest": {
        "laravel/socialite": "To process oauth login for Laravel applications."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xiaohuilam\\Laravel\\SocialiteSkeleton\\SocialiteSkeletonServiceProvider"
            ]
        }
    }
}
